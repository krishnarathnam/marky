{
  "name": "marky",
  "private": true,
  "version": "0.0.4",
  "description": "Marky - simple note-taking app with markdown.",
  "author": "Krishna Rathnam",
  "license": "MIT",
  "type": "module",
  "main": "src/electron/main.js",
  "scripts": {
    "dev": "concurrently -k -r \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "wait-on http://localhost:5173 && cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true electron .",
    "build": "vite build",
    "lint": "eslint .",
    "dist": "npm run build && electron-builder",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.krishna.marky",
    "files": [
      "dist-react/**",
      "src/electron/**"
    ],
    "win": {
      "icon": "assets/icons/icon.ico"
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@benrbray/prosemirror-math": "^1.0.0",
    "@codemirror/lang-markdown": "^6.3.2",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.36.7",
    "@iktakahiro/markdown-it-katex": "^3.1.0",
    "@milkdown/crepe": "^7.9.1",
    "@milkdown/kit": "^7.9.1",
    "@milkdown/plugin-math": "^7.5.9",
    "@milkdown/preset-gfm": "^7.9.1",
    "@milkdown/react": "^7.9.1",
    "@milkdown/theme-nord": "^7.9.1",
    "@tailwindcss/vite": "^4.1.4",
    "@uiw/react-codemirror": "^4.23.12",
    "@uiw/react-heat-map": "^2.3.2",
    "@uiw/react-markdown-editor": "^6.1.2",
    "@uiw/react-markdown-preview": "^5.1.4",
    "@uiw/react-md-editor": "^4.0.6",
    "electron-store": "^10.0.1",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.22",
    "lucide-react": "^0.501.0",
    "markdown-it": "^14.1.0",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-task-lists": "^2.1.1",
    "mermaid": "^11.6.0",
    "motion": "^12.9.4",
    "prismjs": "^1.30.0",
    "prosemirror-example-setup": "^1.2.3",
    "prosemirror-history": "^1.4.1",
    "prosemirror-inputrules": "^1.5.0",
    "prosemirror-keymap": "^1.2.3",
    "prosemirror-model": "^1.25.1",
    "prosemirror-schema-basic": "^1.2.4",
    "prosemirror-schema-list": "^1.5.1",
    "prosemirror-state": "^1.4.3",
    "prosemirror-view": "^1.39.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.5.1",
    "react-router-dom": "^7.5.2",
    "rehype": "^13.0.2",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-directive": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^35.2.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "vite": "^6.3.1",
    "wait-on": "^8.0.3"
  }
}
